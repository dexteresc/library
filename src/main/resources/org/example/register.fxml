<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.Cursor?>
<BorderPane fx:controller="org.example.RegisterController" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" >
<top>
<BorderPane prefHeight="49.0" prefWidth="600.0" styleClass="header" BorderPane.alignment="CENTER">
    <left>
        <Label fx:id="homeButton" text="Bibliotek" textFill="WHITE" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets left="25.0" />
            </BorderPane.margin>
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Label>
    </left>
    <right>
        <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <left>
                <Button fx:id="registerButton" mnemonicParsing="false" styleClass="colorButton" text="Register" BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets left="50.0" />
                    </BorderPane.margin>
                </Button>
            </left>
            <right>
                <Button fx:id="loginButton" mnemonicParsing="false" onAction="#switchToLogin" text="Login" BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets right="25.0" />
                    </BorderPane.margin>
                </Button>
            </right>
        </BorderPane>
    </right>
</BorderPane>
</top>
</BorderPane>